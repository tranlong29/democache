﻿@using UI.Models
@model List<SinhVienModel>
@{
    ViewData["Title"] = "student Page";
}

<div class="text-center">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Tên</th>
                <th scope="col">Địa Chỉ</th>
                <th scope="col">Ngày sinh</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Name</td>
                    <td>@item.DiaChi</td>
                    <td>@item.NgaySinh</td>
                </tr>
            }
        </tbody>
    </table>
</div>